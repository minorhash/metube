<!DOCTYPE html>
<html>
<%include par/head %>
<%include par/nav%>
  <body>

<div class="container-fluid">
<div class="row">
    <!-- row -->
  <div class="col" style="background-color:white">
    <!-- col -->
<div class="card">
  <div class="card-header">
    <h3 align="center"><%= title %></h3>
  </div><!-- head -->

  </div><!-- card -->


   </div><!-- col -->
   </div><!-- row -->

<div class="row">

<div class="col-md-2" style="background-color:lavender">
    <% if(usr){ %>
    <span class="badge badge-info">
        <%= usr!==null %></span>
    <% include par/left %>
    <% include par/out %>
    <% } else { %>
    <span class="badge badge-info">
        no usr</span><br><br>
    <% include par/login %>
    <% } %>


</div><!-- col2 -->

  <div class="col-md-10" style="background-color:white">
<%if(typblo){%>

<table class="table">

    <tr class="table-info"><td>title</td><td>term</td><td>id</td>
    </tr>
<%for (var i=0;i<typblo.length;i++){%>

    <tr><td>      
<form method="post" action="/res-<%=typblo[i].rowid%>">
<input type="hidden" name="id" value="<%= typblo[i].rowid %>">
<input type="submit" class="btn btn-info" value="<%=typblo[i].title %>" >
</form>
  </td><td>
      <%=typblo[i].term%>
  </td><td>

      <%=typblo[i].rowid%>
  </td></tr>
<%}%>
</table>

      <%}else{%>      no post      <%}%>
      <br>

  </div>

    
</div>

    <!-- row end -->
</div>
  </body>
</html>
